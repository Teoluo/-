.card{
  display: flex;
  height: 10vh;
  border-bottom: 1px solid #888;
  align-items: center;
}

.avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #eee;
  margin-left: 25rpx;
}

.avatar image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;

}

.nickName{
  margin-left: 25rpx;
  color: #333;
}

.null{
  display: flex;
  flex-direction: column;
  margin: 400rpx 150rpx;
  text-align: center;
}
